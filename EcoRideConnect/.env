# WARGO Development Environment

# Development Mode - choose SIMPLE_AUTH for in-memory demo, or set DATABASE_URL for Postgres
SIMPLE_AUTH=false
ALLOW_SIMPLE_AUTH_ROUTES=true
VITE_SIMPLE_AUTH=true

# Server Configuration
PORT=5000
NODE_ENV=development
SESSION_SECRET="dev-session-secret-12345"

# Mock Stripe Configuration for Development
STRIPE_SECRET_KEY="sk_test_mock_development_key"
VITE_STRIPE_PUBLISHABLE_KEY="pk_test_mock_development_key"

# Mock Firebase Configuration for Development
VITE_FIREBASE_API_KEY="mock-firebase-api-key"
VITE_FIREBASE_PROJECT_ID="mock-firebase-project"
VITE_FIREBASE_APP_ID="mock-firebase-app-id"

# Google Maps API Key for Development  
# Get your key from: https://console.cloud.google.com/apis/credentials
# Enable Maps JavaScript API and Places API
VITE_GOOGLE_MAPS_API_KEY="your-real-google-maps-api-key-here"

# Database URL (PostgreSQL)
# Example: postgresql://user:password@host:5432/database?sslmode=require
DATABASE_URL="postgresql://user:password@host:5432/ecoride?sslmode=require"